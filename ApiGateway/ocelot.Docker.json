{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/company",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "company.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/company",
      "UpstreamHttpMethod": [ "Get" ],
      "RouteIsCaseSensitive": false,
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 1
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "SwaggerKey": "company"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "company",
      "Config": [
        {
          "Name": "Company API",
          "Version": "v1",
          "Url": "http://company.api:80/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
